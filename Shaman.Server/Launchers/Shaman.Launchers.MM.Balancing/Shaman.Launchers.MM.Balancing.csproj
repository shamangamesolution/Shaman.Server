<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <OutputType>Exe</OutputType>
    </PropertyGroup>


    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.App" />
        <PackageReference Include="Shaman.Bundling.Balancing">
          <Version>1.9.11-remove-router</Version>
        </PackageReference>
        <PackageReference Include="Shaman.Routing.Balancing.Client">
          <Version>1.9.14-remove-router</Version>
        </PackageReference>
        <PackageReference Include="Shaman.Routing.Balancing.Messages">
          <Version>1.9.12-remove-router</Version>
        </PackageReference>
        <PackageReference Include="Shaman.Routing.Balancing.MM">
          <Version>1.9.12-remove-router</Version>
        </PackageReference>
        <ProjectReference Include="..\..\Contracts\Shaman.Contract.Routing.Actualization\Shaman.Contract.Routing.Actualization.csproj" />
      <ProjectReference Include="..\Shaman.Launchers.Common.MM\Shaman.Launchers.Common.MM.csproj" />
    </ItemGroup>


    <ItemGroup>
      <Content Include="appsettings.Development.MatchMaker.json">
        <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      </Content>
      <Content Include="appsettings.launcher.MatchMaker.json">
        <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      </Content>
    </ItemGroup>
    
</Project>
