        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.7.5</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="System.Buffers">
        <Version>4.5.0</Version>
      </PackageReference>
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Buffers\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Buffers\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Buffers\**" />
    </ItemGroup>



</Project>
